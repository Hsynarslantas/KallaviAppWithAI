@model CreateServiceDto
@{
    ViewData["Title"] = "CreateService";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<section class="section">
	<div class="section-body">
		<div class="row">
			<div class="col-12 col-md-12 col-lg-12">
				<div class="card">
					<div class="card-header">
						<h4> Hizmet Giriş Sayfası</h4>
					</div>
					<div class="card-body">
						<form method="post">
							<label>Başlık</label>
							<input type="text" asp-for="Title" class="form-control" />
							<br />
							<label>Özellik</label>
							<input type="text" asp-for="Description" class="form-control" />
							<br />
							<label>İkon Url</label>
							<input type="text" asp-for="IconUrl" class="form-control" />
							<br />
							<button class="btn btn-outline-success">Hizmeti Ekle</button>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>

